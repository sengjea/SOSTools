  "main": "src/main.js",
  "dependencies": {
    "envify": ">=1.2.1",
    "grunt": ">=0.4.4",
    "grunt-browserify": ">=1.3.2",
    "grunt-contrib-watch": ">=0.6.1",
    "grunt-env": ">=0.4.1",
    "react": ">=0.8",
    "react-dom": "15.3.2",
    "underscore": "^1.8.3",
    "express": "^4.x",
    "md5": ">=2.0.0"
  },
  "devDependencies": {
    "grunt-react": "^0.12.3",
    "jsxc": ">=0.1.6"
